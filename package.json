{
	"name": "rollup-plugin-treat",
	"version": "0.0.1-alpha.2",
	"description": "Themeable, statically extracted css-in-js brought to Rollup",
	"keywords": [
		"rollup-plugin",
		"treat",
		"css-in-js",
		"css-in-ts"
	],
	"repository": "maraisr/rollup-plugin-treat",
	"license": "MIT",
	"author": {
		"name": "Marais Rossouw",
		"email": "me@marais.dev",
		"url": "https://marais.io"
	},
	"sideEffects": false,
	"main": "dist/index.js",
	"types": "types/index.d.ts",
	"files": [
		"dist",
		"types"
	],
	"scripts": {
		"build": "tsc --incremental --noEmit false --sourcemap false && tsc -d --declarationDir types/",
		"format": "prettier \"{*,tests/*,lib/*,bench/*,.github/**/*}.+(ts|js|yml|md)\" --write",
		"lint": "tsc --noEmit",
		"prepublishOnly": "yarn run build",
		"test": "uvu -r ts-node/register -i fixtures test"
	},
	"prettier": {
		"bracketSpacing": true,
		"printWidth": 80,
		"proseWrap": "always",
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"dependencies": {
		"@rollup/plugin-commonjs": "^17.1.0",
		"@rollup/plugin-node-resolve": "^11.2.0",
		"@rollup/pluginutils": "^4.1.0",
		"autoprefixer": "^10.2.4",
		"builtin-modules": "^3.2.0",
		"diary": "^0.0.12",
		"eval": "^0.1.6",
		"rollup": "^2.40.0",
		"treat": "^2.0.1"
	},
	"devDependencies": {
		"@marais/tsconfig": "^0.0.1",
		"@types/node": "^14.14.31",
		"polished": "^4.1.1",
		"prettier": "^2.2.1",
		"rimraf": "^3.0.2",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.2",
		"uvu": "^0.5.1"
	},
	"volta": {
		"node": "15.10.0",
		"yarn": "1.22.10"
	}
}
